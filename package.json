{
  "name": "mysql-vcs",
  "displayName": "mysql-vcs",
  "description": "A VSCode extension that allows people to edit their mysql data from VSCode and saving it with a Versioning Control System",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:mysql-vcs.helloWorld",
    "onCommand:mysql-vcs.askQuestion"
  ],
  "main": "./out/src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "mysql-vcs.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "mysql-vcs.askQuestion",
        "category": "MySQL VCS",
        "title": "Ask Question"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.65.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "concurrently": "^7.0.0",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "typescript": "^4.5.5"
  }
}
